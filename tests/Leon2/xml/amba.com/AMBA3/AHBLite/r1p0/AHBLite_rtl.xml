            <ipxact:onSlave>
               <ipxact:direction>out</ipxact:direction>
            </ipxact:onSlave>
         </ipxact:wire>
      </ipxact:port>
      <ipxact:port>
         <ipxact:logicalName>HRUSER</ipxact:logicalName>
         <ipxact:wire>
            <ipxact:onMaster>
               <ipxact:direction>in</ipxact:direction>
            </ipxact:onMaster>
            <ipxact:onSlave>
               <ipxact:direction>out</ipxact:direction>
            </ipxact:onSlave>
         </ipxact:wire>
      </ipxact:port>
      <ipxact:port>
         <ipxact:logicalName>HWUSER</ipxact:logicalName>
         <ipxact:wire>
            <ipxact:onMaster>
               <ipxact:direction>out</ipxact:direction>
            </ipxact:onMaster>
            <ipxact:onSlave>
               <ipxact:direction>in</ipxact:direction>
            </ipxact:onSlave>
         </ipxact:wire>
      </ipxact:port>
      <ipxact:port>
         <ipxact:logicalName>HAUSER</ipxact:logicalName>
         <ipxact:wire>
            <ipxact:onMaster>
               <ipxact:direction>out</ipxact:direction>
            </ipxact:onMaster>
            <ipxact:onSlave>
               <ipxact:direction>in</ipxact:direction>
            </ipxact:onSlave>
         </ipxact:wire>
      </ipxact:port>
   </ipxact:ports>
</ipxact:abstractionDefinition>