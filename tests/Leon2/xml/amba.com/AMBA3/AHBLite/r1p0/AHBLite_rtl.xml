            </ipxact:onMaster>
            <ipxact:onSlave>
               <ipxact:width>1</ipxact:width>
               <ipxact:direction>in</ipxact:direction>
            </ipxact:onSlave>
         </ipxact:wire>
      </ipxact:port>
      <ipxact:port>
         <ipxact:logicalName>HREADYOUT</ipxact:logicalName>
         <ipxact:wire>
            <ipxact:onMaster>
               <ipxact:presence>illegal</ipxact:presence>
            </ipxact:onMaster>
            <ipxact:onSlave>
               <ipxact:width>1</ipxact:width>
               <ipxact:direction>out</ipxact:direction>
            </ipxact:onSlave>
         </ipxact:wire>
      </ipxact:port>
      <ipxact:port>
         <ipxact:logicalName>HRESP</ipxact:logicalName>
         <ipxact:wire>
            <ipxact:onMaster>
               <ipxact:direction>in</ipxact:direction>
