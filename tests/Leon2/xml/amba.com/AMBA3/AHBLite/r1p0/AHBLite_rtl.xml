            </ipxact:onMaster>
            <ipxact:onSlave>
               <ipxact:width>2</ipxact:width>
               <ipxact:direction>in</ipxact:direction>
            </ipxact:onSlave>
         </ipxact:wire>
      </ipxact:port>
      <ipxact:port>
         <ipxact:logicalName>HWRITE</ipxact:logicalName>
         <ipxact:wire>
