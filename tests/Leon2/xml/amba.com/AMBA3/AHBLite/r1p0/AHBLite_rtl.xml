            </ipxact:onMaster>
            <ipxact:onSlave>
               <ipxact:width>3</ipxact:width>
               <ipxact:direction>in</ipxact:direction>
            </ipxact:onSlave>
         </ipxact:wire>
      </ipxact:port>
      <ipxact:port>
         <ipxact:logicalName>HBURST</ipxact:logicalName>
         <ipxact:wire>
            <ipxact:onMaster>
               <ipxact:width>3</ipxact:width>
               <ipxact:direction>out</ipxact:direction>
            </ipxact:onMaster>
            <ipxact:onSlave>
               <ipxact:width>3</ipxact:width>
