   <ipxact:memoryMaps>
      <ipxact:memoryMap>
         <ipxact:name>APB_MM</ipxact:name>
         <ipxact:addressBlock>
            <ipxact:name>defaultid4490597</ipxact:name>
            <ipxact:baseAddress>0</ipxact:baseAddress>
            <ipxact:range>4 * (2 ** 10)</ipxact:range>
            <ipxact:width>width</ipxact:width>
            <ipxact:usage>register</ipxact:usage>
            <ipxact:parameters>
               <ipxact:parameter parameterId="width" type="longint">
                  <ipxact:name>width</ipxact:name>
                  <ipxact:value>32</ipxact:value>
               </ipxact:parameter>
            </ipxact:parameters>
            <ipxact:register>
               <ipxact:name>clock0Divide</ipxact:name>
               <ipxact:addressOffset>'h0</ipxact:addressOffset>
               <ipxact:size>32</ipxact:size>
               <ipxact:volatile>false</ipxact:volatile>
               <ipxact:access>read-write</ipxact:access>
               <ipxact:field>
                  <ipxact:name>Reserved</ipxact:name>
                  <ipxact:description>Reserved</ipxact:description>
                  <ipxact:bitOffset>8</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,8) % $pow(2,24)</ipxact:value>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>24</ipxact:bitWidth>
                  <ipxact:access>read-only</ipxact:access>
               </ipxact:field>
               <ipxact:field>
                  <ipxact:name>divide</ipxact:name>
                  <ipxact:description>Divide value for clock 0, 0=no divide, 1=divide by 2, 2=divide by 4, 3=divide by 6, ...</ipxact:description>
                  <ipxact:bitOffset>0</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,0) % $pow(2,8)</ipxact:value>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>8</ipxact:bitWidth>
                  <ipxact:access>read-write</ipxact:access>
               </ipxact:field>
            </ipxact:register>
            <ipxact:register>
               <ipxact:name>clock1Divide</ipxact:name>
               <ipxact:addressOffset>'h4</ipxact:addressOffset>
               <ipxact:size>32</ipxact:size>
               <ipxact:volatile>false</ipxact:volatile>
               <ipxact:access>read-write</ipxact:access>
