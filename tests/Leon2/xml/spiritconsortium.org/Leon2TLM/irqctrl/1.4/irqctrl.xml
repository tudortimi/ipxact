               <ipxact:parameter parameterId="width" type="longint">
                  <ipxact:name>width</ipxact:name>
                  <ipxact:value>32</ipxact:value>
               </ipxact:parameter>
            </ipxact:parameters>
            <ipxact:register>
               <ipxact:name>interrupMaskandPriorityLevel</ipxact:name>
               <ipxact:description>Interrupt Mask and Priority</ipxact:description>
               <ipxact:addressOffset>'h0</ipxact:addressOffset>
               <ipxact:size>32</ipxact:size>
               <ipxact:access>read-write</ipxact:access>
               <ipxact:field>
                  <ipxact:name>Reserved1</ipxact:name>
                  <ipxact:description>Reserved</ipxact:description>
                  <ipxact:bitOffset>0</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,0) % $pow(2,1)</ipxact:value>
                        <ipxact:mask>('hFFFE) / $pow(2,0) % $pow(2,1)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>1</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
               <ipxact:field>
                  <ipxact:name>imask</ipxact:name>
                  <ipxact:description>Indicates if an interrupt is masked (bit=0) or enabled (bit=1)</ipxact:description>
                  <ipxact:bitOffset>1</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,1) % $pow(2,15)</ipxact:value>
                        <ipxact:mask>('hFFFE) / $pow(2,1) % $pow(2,15)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>15</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
               <ipxact:field>
                  <ipxact:name>Reserved2</ipxact:name>
                  <ipxact:description>Reserved</ipxact:description>
                  <ipxact:bitOffset>16</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,16) % $pow(2,1)</ipxact:value>
                        <ipxact:mask>('hFFFE) / $pow(2,16) % $pow(2,1)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>1</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
               <ipxact:field>
                  <ipxact:name>ilevel</ipxact:name>
                  <ipxact:description>Indicates if an interrupt belongs to priority level 1 (bit=1) or 0 (bit=0)</ipxact:description>
                  <ipxact:bitOffset>17</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,17) % $pow(2,15)</ipxact:value>
                        <ipxact:mask>('hFFFE) / $pow(2,17) % $pow(2,15)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>15</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
            </ipxact:register>
            <ipxact:register>
               <ipxact:name>interruptPendingRegister</ipxact:name>
               <ipxact:description>Indicates if an interrupt is pending</ipxact:description>
               <ipxact:addressOffset>'h4</ipxact:addressOffset>
               <ipxact:size>32</ipxact:size>
               <ipxact:access>read-write</ipxact:access>
               <ipxact:field>
                  <ipxact:name>Reserved1</ipxact:name>
                  <ipxact:description>Reserved</ipxact:description>
                  <ipxact:bitOffset>0</ipxact:bitOffset>
                  <ipxact:bitWidth>1</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
               <ipxact:field>
                  <ipxact:name>ipend</ipxact:name>
                  <ipxact:description>Indicates if an interrupt is pending (bit=1)</ipxact:description>
                  <ipxact:bitOffset>1</ipxact:bitOffset>
                  <ipxact:bitWidth>15</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
               <ipxact:field>
                  <ipxact:name>Reserved2</ipxact:name>
                  <ipxact:description>Reserved</ipxact:description>
                  <ipxact:bitOffset>16</ipxact:bitOffset>
                  <ipxact:bitWidth>16</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
            </ipxact:register>
            <ipxact:register>
               <ipxact:name>interruptForceRegister</ipxact:name>
               <ipxact:description>Indicates if an interrupt is being forced</ipxact:description>
               <ipxact:addressOffset>'h8</ipxact:addressOffset>
               <ipxact:size>32</ipxact:size>
