   </ipxact:busInterfaces>
   <ipxact:memoryMaps>
      <ipxact:memoryMap>
         <ipxact:name>ambaAPB</ipxact:name>
         <ipxact:addressBlock>
            <ipxact:name>defaultid4489914</ipxact:name>
            <ipxact:baseAddress>0</ipxact:baseAddress>
            <ipxact:range>'h28</ipxact:range>
            <ipxact:width>width</ipxact:width>
            <ipxact:parameters>
               <ipxact:parameter parameterId="width" type="longint">
                  <ipxact:name>width</ipxact:name>
                  <ipxact:value>32</ipxact:value>
               </ipxact:parameter>
            </ipxact:parameters>
            <ipxact:register>
               <ipxact:name>watchdogCounter</ipxact:name>
               <ipxact:displayName>watchdogCounter</ipxact:displayName>
               <ipxact:description>Watchdog 1 value</ipxact:description>
               <ipxact:addressOffset>'hc</ipxact:addressOffset>
               <ipxact:size>32</ipxact:size>
               <ipxact:access>read-write</ipxact:access>
               <ipxact:field>
                  <ipxact:name>watchdogValue</ipxact:name>
                  <ipxact:description>Watchdog 1 value</ipxact:description>
                  <ipxact:bitOffset>0</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,0) % $pow(2,24)</ipxact:value>
                        <ipxact:mask>('hFF000000) / $pow(2,0) % $pow(2,24)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>24</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
               <ipxact:field>
                  <ipxact:name>reserved</ipxact:name>
                  <ipxact:description>Reserved</ipxact:description>
                  <ipxact:bitOffset>24</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,24) % $pow(2,8)</ipxact:value>
                        <ipxact:mask>('hFF000000) / $pow(2,24) % $pow(2,8)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>8</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-only</ipxact:access>
                  <ipxact:testable>true</ipxact:testable>
               </ipxact:field>
            </ipxact:register>
            <ipxact:register>
               <ipxact:name>prescalerCounter</ipxact:name>
               <ipxact:displayName>prescalerCounter</ipxact:displayName>
               <ipxact:description>Prescaler Counter</ipxact:description>
               <ipxact:addressOffset>'h20</ipxact:addressOffset>
               <ipxact:size>32</ipxact:size>
               <ipxact:access>read-write</ipxact:access>
               <ipxact:field>
                  <ipxact:name>reloadValue</ipxact:name>
                  <ipxact:description>Prescaler Counter Value</ipxact:description>
                  <ipxact:bitOffset>0</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,0) % $pow(2,10)</ipxact:value>
                        <ipxact:mask>('hFFFFFC00) / $pow(2,0) % $pow(2,10)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>10</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
               <ipxact:field>
                  <ipxact:name>reserved</ipxact:name>
                  <ipxact:description>Reserved</ipxact:description>
                  <ipxact:bitOffset>10</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,10) % $pow(2,22)</ipxact:value>
                        <ipxact:mask>('hFFFFFC00) / $pow(2,10) % $pow(2,22)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>22</ipxact:bitWidth>
                  <ipxact:access>read-only</ipxact:access>
                  <ipxact:testable>true</ipxact:testable>
               </ipxact:field>
            </ipxact:register>
            <ipxact:register>
               <ipxact:name>prescalerReloadValue</ipxact:name>
               <ipxact:displayName>prescalerReloadValue</ipxact:displayName>
               <ipxact:description>Prescaler Reload Value</ipxact:description>
               <ipxact:addressOffset>'h24</ipxact:addressOffset>
               <ipxact:size>32</ipxact:size>
               <ipxact:access>read-write</ipxact:access>
               <ipxact:field>
                  <ipxact:name>reloadValue</ipxact:name>
                  <ipxact:description>Prescaler Reload Value</ipxact:description>
                  <ipxact:bitOffset>0</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,0) % $pow(2,10)</ipxact:value>
                        <ipxact:mask>('hFFFFFC00) / $pow(2,0) % $pow(2,10)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>10</ipxact:bitWidth>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>true</ipxact:testable>
               </ipxact:field>
               <ipxact:field>
                  <ipxact:name>reserved</ipxact:name>
                  <ipxact:description>Reserved</ipxact:description>
                  <ipxact:bitOffset>10</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h0) / $pow(2,10) % $pow(2,22)</ipxact:value>
                        <ipxact:mask>('hFFFFFC00) / $pow(2,10) % $pow(2,22)</ipxact:mask>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>22</ipxact:bitWidth>
                  <ipxact:volatile>true</ipxact:volatile>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:testable>false</ipxact:testable>
               </ipxact:field>
            </ipxact:register>
            <ipxact:registerFile>
               <ipxact:name>timer1</ipxact:name>
               <ipxact:addressOffset>0</ipxact:addressOffset>
               <ipxact:typeIdentifier>timerStruct</ipxact:typeIdentifier>
               <ipxact:range>'h0C</ipxact:range>
               <ipxact:register>
                  <ipxact:name>timerCounter</ipxact:name>
                  <ipxact:displayName>timerCounter</ipxact:displayName>
                  <ipxact:description>Timer value</ipxact:description>
                  <ipxact:addressOffset>'h0</ipxact:addressOffset>
                  <ipxact:size>32</ipxact:size>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:field>
                     <ipxact:name>timerValue</ipxact:name>
                     <ipxact:description>Value of Timer</ipxact:description>
                     <ipxact:bitOffset>0</ipxact:bitOffset>
                     <ipxact:resets>
                        <ipxact:reset>
                           <ipxact:value>('h0) / $pow(2,0) % $pow(2,24)</ipxact:value>
                           <ipxact:mask>('hFF000000) / $pow(2,0) % $pow(2,24)</ipxact:mask>
                        </ipxact:reset>
                     </ipxact:resets>
                     <ipxact:bitWidth>24</ipxact:bitWidth>
                     <ipxact:volatile>true</ipxact:volatile>
                     <ipxact:access>read-write</ipxact:access>
                     <ipxact:testable>true</ipxact:testable>
                  </ipxact:field>
                  <ipxact:field>
                     <ipxact:name>reserved</ipxact:name>
                     <ipxact:description>Reserved</ipxact:description>
                     <ipxact:bitOffset>24</ipxact:bitOffset>
                     <ipxact:resets>
                        <ipxact:reset>
                           <ipxact:value>('h0) / $pow(2,24) % $pow(2,8)</ipxact:value>
                           <ipxact:mask>('hFF000000) / $pow(2,24) % $pow(2,8)</ipxact:mask>
                        </ipxact:reset>
                     </ipxact:resets>
                     <ipxact:bitWidth>8</ipxact:bitWidth>
                     <ipxact:access>read-only</ipxact:access>
                     <ipxact:testable>true</ipxact:testable>
                  </ipxact:field>
               </ipxact:register>
               <ipxact:register>
                  <ipxact:name>timerReload</ipxact:name>
                  <ipxact:displayName>timerReload</ipxact:displayName>
                  <ipxact:description>Timer Reload Value</ipxact:description>
                  <ipxact:addressOffset>'h4</ipxact:addressOffset>
                  <ipxact:size>32</ipxact:size>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:field>
                     <ipxact:name>timerReloadValue</ipxact:name>
                     <ipxact:description>Timer Reload Value</ipxact:description>
                     <ipxact:bitOffset>0</ipxact:bitOffset>
                     <ipxact:resets>
                        <ipxact:reset>
                           <ipxact:value>('h0) / $pow(2,0) % $pow(2,24)</ipxact:value>
                           <ipxact:mask>('hFF000000) / $pow(2,0) % $pow(2,24)</ipxact:mask>
                        </ipxact:reset>
                     </ipxact:resets>
                     <ipxact:bitWidth>24</ipxact:bitWidth>
                     <ipxact:access>read-write</ipxact:access>
                     <ipxact:testable>true</ipxact:testable>
                  </ipxact:field>
                  <ipxact:field>
                     <ipxact:name>reserved</ipxact:name>
                     <ipxact:description>Reserved</ipxact:description>
                     <ipxact:bitOffset>24</ipxact:bitOffset>
                     <ipxact:resets>
                        <ipxact:reset>
                           <ipxact:value>('h0) / $pow(2,24) % $pow(2,8)</ipxact:value>
                           <ipxact:mask>('hFF000000) / $pow(2,24) % $pow(2,8)</ipxact:mask>
                        </ipxact:reset>
                     </ipxact:resets>
                     <ipxact:bitWidth>8</ipxact:bitWidth>
                     <ipxact:access>read-only</ipxact:access>
                     <ipxact:testable>true</ipxact:testable>
                  </ipxact:field>
               </ipxact:register>
               <ipxact:register>
                  <ipxact:name>timerControl</ipxact:name>
                  <ipxact:displayName>timerControl</ipxact:displayName>
                  <ipxact:description>Timer Control Register</ipxact:description>
                  <ipxact:addressOffset>'h8</ipxact:addressOffset>
                  <ipxact:size>32</ipxact:size>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:field>
                     <ipxact:name>enable</ipxact:name>
                     <ipxact:description>Enables the timer when set.</ipxact:description>
                     <ipxact:bitOffset>0</ipxact:bitOffset>
                     <ipxact:resets>
                        <ipxact:reset>
                           <ipxact:value>('h0) / $pow(2,0) % $pow(2,1)</ipxact:value>
                           <ipxact:mask>('hFFFFFFF9) / $pow(2,0) % $pow(2,1)</ipxact:mask>
                        </ipxact:reset>
                     </ipxact:resets>
                     <ipxact:bitWidth>1</ipxact:bitWidth>
                     <ipxact:volatile>true</ipxact:volatile>
                     <ipxact:access>read-write</ipxact:access>
                     <ipxact:testable testConstraint="restore">true</ipxact:testable>
                  </ipxact:field>
                  <ipxact:field>
                     <ipxact:name>reloadCounter</ipxact:name>
                     <ipxact:description>When set to 1 the counter will automatically be reloaded with the reload value after each underflow.</ipxact:description>
                     <ipxact:bitOffset>1</ipxact:bitOffset>
                     <ipxact:resets>
                        <ipxact:reset>
                           <ipxact:value>('h0) / $pow(2,1) % $pow(2,1)</ipxact:value>
                           <ipxact:mask>('hFFFFFFF9) / $pow(2,1) % $pow(2,1)</ipxact:mask>
                        </ipxact:reset>
                     </ipxact:resets>
                     <ipxact:bitWidth>1</ipxact:bitWidth>
                     <ipxact:access>read-write</ipxact:access>
                     <ipxact:testable>true</ipxact:testable>
                  </ipxact:field>
                  <ipxact:field>
                     <ipxact:name>loadCounter</ipxact:name>
                     <ipxact:description>When written with 1, will load the timer reload register into the timer counter register. Always reads as a 0.</ipxact:description>
                     <ipxact:bitOffset>2</ipxact:bitOffset>
                     <ipxact:resets>
                        <ipxact:reset>
                           <ipxact:value>('h0) / $pow(2,2) % $pow(2,1)</ipxact:value>
                           <ipxact:mask>('hFFFFFFF9) / $pow(2,2) % $pow(2,1)</ipxact:mask>
                        </ipxact:reset>
                     </ipxact:resets>
                     <ipxact:bitWidth>1</ipxact:bitWidth>
                     <ipxact:access>read-write</ipxact:access>
                     <ipxact:modifiedWriteValue>oneToClear</ipxact:modifiedWriteValue>
                     <ipxact:testable>true</ipxact:testable>
                  </ipxact:field>
                  <ipxact:field>
                     <ipxact:name>reserved</ipxact:name>
                     <ipxact:description>Reserved</ipxact:description>
                     <ipxact:bitOffset>3</ipxact:bitOffset>
                     <ipxact:resets>
                        <ipxact:reset>
                           <ipxact:value>('h0) / $pow(2,3) % $pow(2,29)</ipxact:value>
                           <ipxact:mask>('hFFFFFFF9) / $pow(2,3) % $pow(2,29)</ipxact:mask>
                        </ipxact:reset>
                     </ipxact:resets>
                     <ipxact:bitWidth>29</ipxact:bitWidth>
                     <ipxact:access>read-only</ipxact:access>
                     <ipxact:testable>true</ipxact:testable>
                  </ipxact:field>
               </ipxact:register>
            </ipxact:registerFile>
            <ipxact:registerFile>
               <ipxact:name>timer2</ipxact:name>
               <ipxact:addressOffset>'h10</ipxact:addressOffset>
               <ipxact:typeIdentifier>timerStruct</ipxact:typeIdentifier>
               <ipxact:range>'h0C</ipxact:range>
               <ipxact:register>
                  <ipxact:name>timerCounter</ipxact:name>
                  <ipxact:displayName>timerCounter</ipxact:displayName>
                  <ipxact:description>Timer value</ipxact:description>
                  <ipxact:addressOffset>'h0</ipxact:addressOffset>
                  <ipxact:size>32</ipxact:size>
                  <ipxact:access>read-write</ipxact:access>
                  <ipxact:field>
                     <ipxact:name>timerValue</ipxact:name>
                     <ipxact:description>Value of Timer</ipxact:description>
                     <ipxact:bitOffset>0</ipxact:bitOffset>
                     <ipxact:resets>
                        <ipxact:reset>
