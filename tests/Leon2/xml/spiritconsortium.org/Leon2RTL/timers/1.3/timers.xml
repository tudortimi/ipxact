         <ipxact:abstractionTypes>
            <ipxact:abstractionType>
               <ipxact:abstractionRef vendor="spiritconsortium.org" library="busdef.interrupt" name="interrupt_rtl"
                                      version="1.0"/>
               <ipxact:portMaps>
                  <ipxact:portMap>
                     <ipxact:logicalPort>
                        <ipxact:name>IRQ</ipxact:name>
                     </ipxact:logicalPort>
