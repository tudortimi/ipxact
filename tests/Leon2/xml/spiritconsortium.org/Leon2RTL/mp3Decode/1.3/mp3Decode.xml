                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>8</ipxact:bitWidth>
                  <ipxact:access>read-write</ipxact:access>
               </ipxact:field>
            </ipxact:register>
            <ipxact:register>
               <ipxact:name>IDReg</ipxact:name>
               <ipxact:description>ID register</ipxact:description>
               <ipxact:addressOffset>'hFFC</ipxact:addressOffset>
               <ipxact:size>32</ipxact:size>
               <ipxact:access>read-only</ipxact:access>
               <ipxact:field>
                  <ipxact:name>ID</ipxact:name>
                  <ipxact:description>ID Field</ipxact:description>
                  <ipxact:bitOffset>0</ipxact:bitOffset>
                  <ipxact:resets>
                     <ipxact:reset>
                        <ipxact:value>('h00000D04) / $pow(2,0) % $pow(2,32)</ipxact:value>
                     </ipxact:reset>
                  </ipxact:resets>
                  <ipxact:bitWidth>32</ipxact:bitWidth>
                  <ipxact:access>read-only</ipxact:access>
               </ipxact:field>
            </ipxact:register>
         </ipxact:addressBlock>
