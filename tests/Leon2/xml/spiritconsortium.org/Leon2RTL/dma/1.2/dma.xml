      <ipxact:memoryMap>
         <ipxact:name>ambaAPB</ipxact:name>
         <ipxact:addressBlock>
            <ipxact:name>defaultid4490081</ipxact:name>
            <ipxact:baseAddress>0</ipxact:baseAddress>
